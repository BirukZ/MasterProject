/* Printing Style*/
@media print{


    div#form1\:Header\:headerLayout{
        display:none;

    }
    div#form1\:whole_page\:page_content\:page_left{
        display: none;
        font-size: 12px;
    }
    body\#body1\#form1\:whole_page{
        font-size: 12px;
        background:black;
        background-image:none;
        border:none;
        display:block;
    }

    div#form1\:footer{
        display: none;
    }

    div#form1\:whole_page\:page_content\:main_content{
        margin-left: 10px;
        position: absolute;
    }
    .hide_on_print{
        display: none;
    }
    .show_on_print{
        display: block;
    }
    .on_print_border{
        display: block;
        border-top: 0px;
        border-right: 0px;
        border-left: 0px;
        border-bottom: solid black 1px;
    }
}


















